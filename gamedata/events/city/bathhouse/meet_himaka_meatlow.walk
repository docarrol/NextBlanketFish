self.addStat(&quot;generic.calories&quot;, 50000);&#xd;
self.addStat(&quot;abdomen.food&quot;, 10000);&#xd;
SetPerFlag( &quot;himanka_eaten&quot;,(GetPerFlag( &quot;himanka_eaten&quot;)+50));&#xd;
&quot;&quot;;%&gt;&#xd;
