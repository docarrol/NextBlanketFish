self.addStat(&quot;generic.calories&quot;, 100000);&#xd;
self.addStat(&quot;abdomen.food&quot;, 5000);&#xd;
SetPerFlag(&quot;dwarfparty_eat&quot;, 0);&#xd;
SetPerFlag(&quot;partycurtain&quot;, 0);&#xd;
&quot;&quot;;%&gt;</descr>
